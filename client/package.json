{
  "name": "netloier-ui",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bunx --bun vite\" \"bun run watch-tailwind\"",
    "build": "tsc && vite build",
    "start": "vite preview",
    "watch-tailwind": "bun run tailwindcss build -i ./src/main.css -o ./dist/output.css --watch"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.10",
    "@mui/styles": "^5.15.10",
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.45.1",
    "@trpc/react-query": "^10.45.1",
    "@trpc/server": "^10.45.1",
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "react-use-websocket": "^4.7.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@vitejs/plugin-react": "^3.1.0",
    "concurrently": "8.2.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^4.1.2"
  },
  "bun": {
    "entryPoints": ["src/main.tsx"]
  }
}
